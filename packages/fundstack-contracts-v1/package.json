  "scripts": {
    "watch": "nodemon --watch src -e py --exec \"python3 src/contracts/v1/fund/main.py & python3 src/contracts/v1/platform/main.py & npm run compile-teal\"",
    "build": "npm run build:clean && npm run build:cjs",
    "build:clean": "rimraf dist/",
    "build:cjs": "tsc -p ./tsconfig-cjs.json",
    "compile-teal": "NODE_ENV=development npm run build && node ./dist/cjs/compileTeal.js",
    "deploy-platform-application": "NODE_ENV=development npm run build && node ./dist/cjs/deployPlatformApplication.js",
    "update-platform-application": "NODE_ENV=development npm run build && node ./dist/cjs/updatePlatformApplication.js"
  },
